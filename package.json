{
  "name": "gitlab-upm-proxy",
  "version": "0.2.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "cross-env DOTENV_CONFIG_PATH=test/.env.test npm run build:ts && cross-env NODE_OPTIONS=--import=dotenv/config tsx --test test/routes/gitlab-npm-proxy.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "AmariNoa",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/sensible": "^6.0.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.1",
    "fastify-plugin": "^5.0.0",
    "semver": "^7.7.2",
    "tar": "^7.5.1",
    "undici": "^7.18.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.8",
    "@types/semver": "^7.7.1",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.9.2"
  }
}
